{
    "name": "postyou/contao-venobox-bundle",
    "type": "contao-bundle",
    "description": "Integrate Venobox in Contao",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Mario Gienapp",
            "homepage": "https://github.com/theDyingMountain"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core-bundle": "^4.4"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.6",
        "leofeyer/optimize-native-functions-fixer": "^1.1",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "Postyou\\ContaoPageToAjaxBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Postyou\\ContaoVenoboxBundle\\": "src/"
        }
    },
    "support": {
        "issues": "https://github.com/postyou/contao-venobox-bundle/issues",
        "source": "https://github.com/postyou/contao-venobox-bundle"
    }
}
